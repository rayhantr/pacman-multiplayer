{
  "name": "pacman-typescript",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "dev": "nodemon --exec ts-node src/server/index.ts",
    "build:client": "tsc --project tsconfig.client.json",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit && tsc --project tsconfig.client.json --noEmit",
    "quality": "npm run type-check && npm run lint"
  },
  "keywords": [
    "pacman",
    "multiplayer",
    "typescript",
    "websocket"
  ],
  "author": "",
  "license": "ISC",
  "description": "Multiplayer Pac-Man game built with TypeScript",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.7",
    "@types/socket.io-client": "^1.4.36",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  }
}
